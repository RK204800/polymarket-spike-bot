{
  "$schema": "https://railway.app/schema.json",
  "build": {
    "builder": "NIXPACKS_PYTHON",
    "python": {
      "version": "3.11",
      "installCmd": "pip install -r requirements.txt"
    }
  },
  "deploy": {
    "numReplicas": 1,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "startCommand": "python main.py"
  }
}
